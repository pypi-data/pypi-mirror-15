boatd:
    port: 2222
    interface: 127.0.0.1

plugin_directory: null

plugins:
    - logger:
        enabled: false
        period: 10
        filename: logs/log_trace

    - mavlink:
        enabled: false
        device: /dev/ttyUSB0
        baud: 115200

driver:
    file: example/basic_driver.py

behaviours:
    - example:
        file: example/basic_behaviour.py
