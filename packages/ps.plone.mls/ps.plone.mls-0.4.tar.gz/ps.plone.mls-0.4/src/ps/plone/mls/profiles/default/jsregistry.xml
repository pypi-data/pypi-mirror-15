<?xml version="1.0"?>
<object name="portal_javascripts">

  <!-- MLS JS. -->
  <!-- ======= -->
  <javascript
      cacheable="True"
      compression="safe"
      cookable="True"
      enabled="True"
      id="++resource++ps.plone.mls/mls.js"
      />


  <!-- Google Maps JS. -->
  <!-- =============== -->

  <!-- HTTP support. -->
  <javascript
      compression="none"
      cookable="False"
      enabled="True"
      expression="python:not context.absolute_url().startswith('https')"
      id="http://maps.google.com/maps/api/js?sensor=false"
      inline="False"
      />

  <!-- HTTPS support. -->
  <javascript
      compression="none"
      cookable="False"
      enabled="True"
      expression="python:context.absolute_url().startswith('https')"
      id="https://maps-api-ssl.google.com/maps/api/js?sensor=false"
      inline="False"
      insert-after="http://maps.google.com/maps/api/js?sensor=false"
      />

</object>
