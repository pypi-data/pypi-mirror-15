import sys, time, pyAMI.app, pyAMI_supernemo.config

#############################################################################
# ENTRY POINT                                                               #
#############################################################################

def entry_point():
	app = pyAMI.app.App('supernemo', 'text')

	return app.run()

#############################################################################

if __name__ == '__main__':
	sys.exit(entry_point())

