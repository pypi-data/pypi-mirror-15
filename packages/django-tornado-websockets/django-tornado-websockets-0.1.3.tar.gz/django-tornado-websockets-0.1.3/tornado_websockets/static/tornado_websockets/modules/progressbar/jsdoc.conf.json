{
  "source": {
    "include": [
      "package.json",
      "dist/engine.js",
      "dist/engine_bootstrap.js",
      "dist/engine_html5.js",
      "README.md",
      "LICENSE"
    ]
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "opts": {
    "destination": "./doc/",
    "encoding": "utf8",
    "template": "./node_modules/jaguarjs-jsdoc",
    "private": true
  }
}
