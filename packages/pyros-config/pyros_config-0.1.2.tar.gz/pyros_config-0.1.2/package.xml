<?xml version="1.0"?>
<package format="2">
  <!--
   This file is mostly here to provide ros dependencies when this project is built with catkin
   -->
  <name>pyros_config</name>
  <version>0.1.2</version>
  <description>
  Configuration package for Pyros
  </description>

  <license>BSD</license>

  <url type="repository">https://github.com/asmodehn/pyros-config</url>
  <url type="bugtracker">https://github.com/asmodehn/pyros-config/issues</url>

  <author email="asmodehn@gmail.com">AlexV</author>
  <maintainer email="asmodehn@gmail.com">AlexV</maintainer>

  <buildtool_depend version_gte="0.6.18">catkin</buildtool_depend>
  <build_depend version_gte="0.1.3">catkin_pip</build_depend>

  <export>
    <rosdoc config="rosdoc.yaml" />
  </export>

</package>


