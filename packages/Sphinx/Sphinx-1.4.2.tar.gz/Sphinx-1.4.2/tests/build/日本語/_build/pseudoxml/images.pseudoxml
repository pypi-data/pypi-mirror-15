<document source="/Users/tkomiya/work/sphinx/tests/build/日本語/images.txt">
    <section ids="sphinx-image-handling" names="sphinx\ image\ handling">
        <title>
            Sphinx image handling
        <comment xml:space="preserve">
            first, a simple test with direct filename
        <image candidates="{'*': u'img.png'}" uri="img.png">
        <comment xml:space="preserve">
            a non-existing image with direct filename
        <image candidates="{'*': u'foo.png'}" uri="foo.png">
        <comment xml:space="preserve">
            an image with path name (relative to this directory!)
        <image candidates="{'*': u'subdir/img.png'}" height="100" uri="subdir/img.png" width="200">
        <comment xml:space="preserve">
            an image with unspecified extension
        <image candidates="{'image/gif': u'img.gif', 'image/png': u'img.png', 'application/pdf': u'img.pdf'}" uri="img.*">
        <comment xml:space="preserve">
            a non-existing image with .*
        <image candidates="{}" uri="foo.*">
        <comment xml:space="preserve">
            a non-local image URI
        <image candidates="{'?': u'http://www.python.org/logo.png'}" uri="http://www.python.org/logo.png">
        <comment xml:space="preserve">
            an image with subdir and unspecified extension
        <image candidates="{'image/png': u'subdir/simg.png'}" uri="subdir/simg.*">
        <comment xml:space="preserve">
            an SVG image (for HTML at least)
        <image candidates="{'application/pdf': u'svgimg.pdf', 'image/svg+xml': u'svgimg.svg'}" uri="svgimg.*">
        <comment xml:space="preserve">
            an image with more than 1 dot in its file name
        <image candidates="{'*': u'img.foo.png'}" uri="img.foo.png">
        <substitution_definition names="subst">
            global substitution
